<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Scene?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<Scene xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1">
            <AnchorPane prefHeight="200" prefWidth="261.0" style="-fx-background-color: ffffff;">
            <children>
               <VBox alignment="CENTER" layoutX="65.0" prefHeight="200.0" prefWidth="131.0" spacing="20.0">
                  <children>
                     <Label text="WELCOME">
                        <font>
                           <Font name="Book Antiqua" size="20.0" />
                        </font>
                     </Label>
                     <VBox alignment="CENTER" prefHeight="112.0" prefWidth="97.0" spacing="10.0">
                        <children>
                           <Button fx:id="open" mnemonicParsing="false" prefHeight="35.0" prefWidth="132.0" style="-fx-background-color: #bff4c6;" text="Open existing bujo">
                              <font>
                                 <Font name="Book Antiqua" size="10.0" />
                              </font>
                           </Button>
                           <Button fx:id="create" mnemonicParsing="false" prefHeight="32.0" prefWidth="132.0" style="-fx-background-color: #bff4c6;" text="Create new Bujo">
                              <font>
                                 <Font name="Book Antiqua" size="10.0" />
                              </font>
                           </Button>
                     <Button fx:id="template" mnemonicParsing="false" prefHeight="31.0" prefWidth="132.0" style="-fx-background-color: #bff4c6;" text="New Bujo from Template">
                        <font>
                           <Font name="Book Antiqua" size="10.0" />
                        </font>
                     </Button>
                        </children>
                     </VBox>
                  </children>
               </VBox>
            </children></AnchorPane>
        </Scene>

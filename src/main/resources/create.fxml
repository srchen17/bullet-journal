<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.DialogPane?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<DialogPane fx:id="upload" prefHeight="400.0" prefWidth="300.0" stylesheets="@style.css" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <content>
      <AnchorPane prefHeight="400.0" prefWidth="300.0">
         <children>
            <VBox alignment="CENTER" layoutX="19.0" layoutY="11.0" prefHeight="400.0" prefWidth="300.0" spacing="10.0">
               <children>
                  <Label alignment="CENTER" prefHeight="43.0" prefWidth="174.0" text="Enter the week name" textFill="WHITE">
                     <font>
                        <Font name="Book Antiqua" size="16.0" />
                     </font>
                  </Label>
                  <TextField fx:id="name">
                     <opaqueInsets>
                        <Insets top="10.0" />
                     </opaqueInsets>
                  </TextField>
                  <PasswordField fx:id="passwordField" promptText="Create password" />
                  <Button fx:id="enter" alignment="TOP_CENTER" mnemonicParsing="false" prefHeight="22.0" prefWidth="60.0" style="-fx-background-color: transparent;" text="Submit" textFill="WHITE">
                     <opaqueInsets>
                        <Insets top="10.0" />
                     </opaqueInsets>
                     <font>
                        <Font name="Book Antiqua" size="13.0" />
                     </font>
                  </Button>
                  <Label fx:id="userWarning" alignment="CENTER" contentDisplay="CENTER" prefHeight="17.0" prefWidth="192.0" textFill="WHITE">
                     <font>
                        <Font name="Heiti TC Light" size="10.0" />
                     </font>
                  </Label>
               </children>
               <padding>
                  <Insets left="10.0" right="100.0" top="10.0" />
               </padding>
            </VBox>
         </children>
      </AnchorPane>
   </content>
</DialogPane>
